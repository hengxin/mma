  RowBox[{"Expand", "[", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", "+", "x", "+", "y"}], ")"}], "3"], "]"}]}]], "Input",
 CellTags->"Polynomials",
 CellLabel->"",ExpressionUUID->"44ff49c5-031a-4f20-bef0-ac1b70d48352"],

